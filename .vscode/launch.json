{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}"
			],
			"stopOnEntry": false,
			"sourceMaps": true
		},
		{
			"name": "Attach to Debugger",
			"type": "clr",
			"request": "attach",
			"processName": "UnityDebug.exe",
			"justMyCode": false,
			"requireExactSource": false,
		}
	]
}